{
  "name": "ContentSettings Example",
  "description": "ContentSettings Example",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
"permissions": [
    "contentSettings"
  ],
  "host_permissions": [
    "https://example.com/*",
    "https://www.openfin.co/*",
    "http://localhost:8081/*",
    "https://*.microsoft.com/*"
  ]
}