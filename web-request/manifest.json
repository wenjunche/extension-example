{
  "name": "webRequest Example",
  "description": "webRequest Example",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
                  "webRequest",
                  "tabs",
                  "storage"
                 ],
  "host_permissions": [
                  "<all_urls>"
                ],
  "action": {
      "default_popup": "popup.html"
  }                
}
