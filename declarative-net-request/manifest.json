{
  "name": "DeclarativeNetRequest Example",
  "description": "DeclarativeNetRequest Example",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],
  "host_permissions": [
    "https://example.com/*",
    "https://www.openfin.co/*",
    "http://localhost:8081/*"
  ],
"declarative_net_request": {
    "rule_resources": [
      {
        "id": "1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  }
}